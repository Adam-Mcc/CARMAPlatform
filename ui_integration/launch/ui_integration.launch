<!-- 
Primary launch file for UI Integration node
Loads parameters and configures logging for node
 -->
<launch>
    <!-- UI Integration Node -->
    <node pkg="ui_integration" type="ui_integration_node" name="ui_integration_node" output="screen">
        <rosparam command="load" file="$(find ui_integration)/config/params.yaml" />
    </node>
</launch>
